}

@UNPUBLISHED{Sun2020-zv,
  title    = "A Review on the Factors Contributing to {2019-nCoV} Virus
              Outbreaks in Wuhan",
  author   = "Sun, Zhong and Karuppiah, Thilakavathy and Kumar, S Suresh and
              He, Guozhong",
  abstract = "The new coronavirus (2019-nCoV) in Wuhan has caused virus
              outbreaks in many provinces and cities in China, and several
              neighboring countries were also affected. In recent years,
              coronavirus several outbreaks around the world were reported,
