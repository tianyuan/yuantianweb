@ARTICLE{Kobayashi2020-lu,
  title    = "Communicating the Risk of Death from Novel Coronavirus Disease
              ({COVID-19})",
  author   = "Kobayashi, Tetsuro and Jung, Sung-Mok and Linton, Natalie M and
