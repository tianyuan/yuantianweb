  volume   =  18,
  number   =  2,
  pages    = "147--160",
  month    =  mar,
  year     =  2015,
  keywords = "decision making; dynamic simulation modeling; health care
              delivery; methods;model evaluation;Good Practice",
  language = "en"
}

@ARTICLE{Berg2010-rn,
