  language = "en"
}

@ARTICLE{Lee2010-iq,
  title    = "A computer simulation of employee vaccination to mitigate an
              influenza epidemic",
  author   = "Lee, Bruce Y and Brown, Shawn T and Cooley, Philip C and
              Zimmerman, Richard K and Wheaton, William D and Zimmer, Shanta M
              and Grefenstette, John J and Assi, Tina-Marie and Furphy, Timothy
              J and Wagener, Diane K and Burke, Donald S",
  abstract = "BACKGROUND: Better understanding the possible effects of
              vaccinating employees is important and can help policymakers and
              businesses plan vaccine distribution and administration
              logistics, especially with the current H1N1 influenza vaccine in
              short supply. PURPOSE: This article aims to determine the effects
              of varying vaccine coverage, compliance, administration rates,
