@ARTICLE{Milstein2010-rs,
  title    = "Analyzing national health reform strategies with a dynamic
              simulation model",
  author   = "Milstein, Bobby and Homer, Jack and Hirsch, Gary",
  abstract = "Proposals to improve the US health system are commonly supported
